        "ping": "^0.4.2",
        "request": "^2.88.2",
        "sequelize": "^6.28.0",
        "sqlite3": "^5.1.4",
        "string-similarity": "^4.0.4",
        "yahoo-finance": "^0.3.7"
    },
    "devDependencies": {
        "eslint": "^8.30.0",
