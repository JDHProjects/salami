    "main": "index.js",
    "author": "Joshua Purdy",
    "scripts": {
        "start": "nodemon index.js",
        "unit-test-env": "ENVIRONMENT=localhost TEST=TRUE PREFIX=, DOWN_DETECTION=FALSE nodemon index.js",
        "localhost": "ENVIRONMENT=localhost PREFIX=, DOWN_DETECTION=FALSE nodemon index.js",
